solution_module = Smaoc.Solution
solution_module = Smaoc.Solution.Day4

defmodule solution_module do
  defp parse_input(input) do
    input
    |> String.split("\n")
    |> Enum.reject(&(&1 == ""))
    |> Enum.map(&parse_line/1)

    # |> dbg
  end

  defp parse_line(text) do
    ~r/:(.*)\|(.*)/
    |> Regex.scan(text)
    |> then(fn [[_, w, c]] ->
      {fetch_numbers(w), fetch_numbers(c)}
    end)
  end

  defp fetch_numbers(text) do
    ~r/\d+/
    |> Regex.scan(text)
    |> Enum.map(fn [n] -> String.to_integer(n) end)
  end

  defp calc_wins({w, c}) do
    c
    |> Enum.filter(&(&1 in w))
    |> Enum.count()
  end

  defp calc_points(card) do
    card
    |> calc_wins
    |> case do
      0 -> 0
      x -> 2 ** (x - 1)
    end
  end

  def solve(:part1, input) do
    input
    |> parse_input()
    |> Enum.map(&calc_points/1)
    |> Enum.sum()
  end

  def solve(:part2, input) do
    cards = parse_input(input)
    m = Enum.reduce(0..(length(cards) - 1), %{}, &Map.put(&2, &1, 1))

    cards
    |> Enum.with_index()
    |> Enum.reduce(m, fn {card, idx}, acc ->
      cur_count = Map.get(acc, idx)

      card
      |> calc_wins()
      |> case do
        0 -> []
        wins -> (idx + 1)..(idx + wins)
      end
      |> Enum.reduce(acc, &Map.update!(&2, &1, fn x -> x + cur_count end))
    end)
    |> Enum.map(fn {_, v} -> v end)
    |> Enum.sum()
  end
end


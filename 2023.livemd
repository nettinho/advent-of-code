defmodule solution_module do
  defp int(x), do: String.to_integer(x)

  defp parse_input(input, part) do
    input
    |> String.split(":")
    |> then(fn [_, seeds | categories] ->
      {
        parse_seeds(part, seeds),
        categories |> Enum.map(&parse_category/1) |> Enum.reverse()
      }
    end)
  end

  defp parse_seeds(:part1, text) do
    ~r/\d+/
    |> Regex.scan(text)
    |> Enum.map(fn [x] -> {int(x), int(x)} end)
  end

  defp parse_seeds(:part2, text) do
    ~r/(\d+) (\d+)/

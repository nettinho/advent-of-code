solution_module = Smaoc.Solution
solution_module = Smaoc.Solution.Day1

defmodule solution_module do
  defp parse_input(input) do
    input |> String.split("\n") |> Enum.reject(&(&1 == ""))
  end

  def solve(:part1, input) do
    parse_input(input)
    |> Enum.map(&make_number/1)
    |> Enum.reject(&is_nil/1)
    |> Enum.sum()

    # |> dbg
  end

  def solve(:part2, input) do
    parse_input(input)
    |> Enum.map(&solve2/1)
    |> Enum.sum()
  end

  defp solve2(text) do
    n1 = first(text) |> translate()
    n2 = second(text) |> translate()
    String.to_integer("#{n1}#{n2}")
  end

  defp first(text), do: Regex.run(~r/(one|two|three|four|five|six|seven|eight|nine|\d)/, text)
  defp second(text), do: Regex.run(~r/.*(one|two|three|four|five|six|seven|eight|nine|\d)/, text)

  defp translate([_, "one"]), do: "1"
  defp translate([_, "two"]), do: "2"
  defp translate([_, "three"]), do: "3"
  defp translate([_, "four"]), do: "4"
  defp translate([_, "five"]), do: "5"
  defp translate([_, "six"]), do: "6"
  defp translate([_, "seven"]), do: "7"
  defp translate([_, "eight"]), do: "8"
  defp translate([_, "nine"]), do: "9"
  defp translate([_, t]), do: t

  defp make_number(text) do
    text
    |> regex1()
    |> regex2(text)
    |> case do
      [_, num1, num2] -> String.to_integer("#{num1}#{num2}")
      [_, num] -> String.to_integer("#{num}#{num}")
      _ -> nil
    end

    # |> dbg()
  end

  defp regex1(text), do: Regex.run(~r/[\D]*(\d).*(\d)[\D]*/, text)

  defp regex2([_, _, _] = parsed, _), do: parsed
  defp regex2(_, text), do: Regex.run(~r/[\D]*(\d)[\D]*/, text)
end


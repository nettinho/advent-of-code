        seed
      else
        calculate_min({seeds, cats}, milis, seed + 1)
      end
    end)
  end

  defp reduce_seed(cat, seed) do
    cat
    |> Enum.drop_while(fn [dest, _, rng] -> seed not in dest..(dest + rng - 1) end)
    |> case do
      [[dest, src, _] | _] -> src + seed - dest
      _ -> seed
    end
  end

  def solve(:part1, input) do
    input
    |> parse_input(:part1)
    |> calculate_min(:os.system_time(:millisecond))

    # |> dbg
  end

  def solve(:part2, input) do
    input
    |> parse_input(:part2)
    |> calculate_min(:os.system_time(:millisecond))
  end
end


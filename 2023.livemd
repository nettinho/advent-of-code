```elixir
solution_module = Smaoc.Solution
solution_module = Smaoc.Solution.Day3

defmodule solution_module do
  defp parse_input(input) do
    input
    |> String.split("\n")
    |> Enum.reject(&(&1 == ""))
    |> Enum.map(&parse_line/1)
    |> Enum.with_index()
    |> Enum.reduce(%{m: [], s: [], g: []}, &reduce_line/2)

    # |> dbg
  end

  defp parse_line(text) do
    ~r/(?:\.|\d+|[^\.\d])/
    |> Regex.scan(text)
    |> Enum.reduce([%{m: [], s: [], g: []}, 0], &reduce_token/2)
    |> Enum.at(0)
  end

  defp reduce_token(["."], [m, c]), do: [m, c + 1]

  defp reduce_token(["*"], [%{m: m, s: s, g: g}, c]),
    do: [%{m: m, s: [c | s], g: [c | g]}, c + 1]

  defp reduce_token([text], [%{m: m, s: s, g: g}, c]) do
    case Integer.parse(text) do
      :error -> [%{m: m, s: [c | s], g: g}, c + 1]
      {n, _} -> [%{m: [[c, n] | m], s: s, g: g}, c + String.length(text)]
    end
  end

  defp reduce_line({%{m: m, s: s, g: g}, y}, %{m: am, s: as, g: ag}) do
    nm = Enum.map(m, fn [x, n] -> [x, y, n] end)
    ns = Enum.map(s, fn x -> [x, y] end)
    ng = Enum.map(g, fn x -> [x, y] end)
    %{m: am ++ nm, s: as ++ ns, g: ag ++ ng}
  end

  defp nlen(0), do: 1
  defp nlen(x), do: trunc(:math.log10(x) + 1)

  defp has_symbol([x, y, n], s) do
    for xx <- -1..nlen(n) do
      for yy <- -1..1 do
        {xx, yy}
      end
    end
    |> Enum.flat_map(& &1)
    |> Enum.any?(fn {sx, sy} -> [x + sx, y + sy] in s end)
  end

  defp gear_machines([xx, yy], m) do
    for x <- (xx - 1)..(xx + 1) do
      for y <- (yy - 1)..(yy + 1) do
        {x, y}
      end
    end
    |> Enum.flat_map(& &1)
    |> Enum.map(fn {x, y} ->
      Enum.find(m, nil, fn
        [^x, ^y, _, idx] -> idx
        _ -> nil
      end)
    end)
    |> Enum.reject(&is_nil/1)
    |> Enum.uniq_by(fn [_, _, _, idx] -> idx end)
    |> Enum.map(fn [_, _, n, _] -> n end)
  end

  def solve(:part1, input) do
    %{m: m, s: s} = parse_input(input)

    m
    |> Enum.filter(&has_symbol(&1, s))
    |> Enum.map(fn [_, _, n] -> n end)
    |> Enum.sum()
  end

  def solve(:part2, input) do
    %{m: m, g: g} = parse_input(input)

    m_pos =
      m
      |> Enum.with_index()
      |> Enum.flat_map(fn {[x, y, n], idx} ->
        for xx <- x..(x + nlen(n) - 1) do
          [xx, y, n, idx]
        end
      end)

    g
    |> Enum.map(&gear_machines(&1, m_pos))
    |> Enum.map(fn
      [n1, n2] -> n1 * n2
      _ -> 0
    end)
    |> Enum.sum()
  end
end


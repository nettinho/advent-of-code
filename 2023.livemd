solution_module = Smaoc.Solution
solution_module = Smaoc.Solution.Day2

defmodule solution_module do
  defp parse_input(input) do
    input
    |> String.split("\n")
    |> Enum.reject(&(&1 == ""))
    |> Enum.map(&parse_line/1)
  end

  defp parse_line(text) do
    ["Game " <> id, plays] = String.split(text, ":")

    plays
    |> String.split(";")
    |> Enum.flat_map(&parse_play/1)
    |> Enum.reduce(%{id: String.to_integer(id), r: 0, g: 0, b: 0}, &reduce_max/2)
  end

  defp parse_play(text) do
    text
    |> String.split(",")
    |> Enum.map(&parse_color/1)
  end

  defp parse_color(text) do
    text
    |> String.trim()
    |> then(&Regex.run(~r/(\d*) ([rgb])/, &1))
    |> then(fn [_, count, color] -> {color, String.to_integer(count)} end)
  end

  def reduce_max({"r", count}, %{r: cur} = acc) when count > cur, do: Map.put(acc, :r, count)
  def reduce_max({"g", count}, %{g: cur} = acc) when count > cur, do: Map.put(acc, :g, count)
  def reduce_max({"b", count}, %{b: cur} = acc) when count > cur, do: Map.put(acc, :b, count)
  def reduce_max(_, acc), do: acc

  def solve(:part1, input) do
    input
    |> parse_input()
    |> Enum.filter(fn %{r: r, g: g, b: b} ->
      r < 13 and g < 14 and b < 15
    end)
    |> Enum.map(& &1.id)
    |> Enum.sum()

    # |> dbg
  end

  def solve(:part2, input) do
    input
    |> parse_input()
    |> Enum.map(fn %{r: r, g: g, b: b} ->
      r * g * b
    end)
    |> Enum.sum()
  end
end


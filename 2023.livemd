    |> Enum.map(fn [_, x, rng] -> {int(x), int(x) + int(rng) - 1} end)
  end

  defp parse_category(text) do
    ~r/(\d+) (\d+) (\d+)/
    |> Regex.scan(text)
    |> Enum.map(fn [_ | m] ->
      Enum.map(m, &int/1)
    end)
  end

  defp in_seeds?(x, seeds) do
    Enum.any?(seeds, fn {n1, n2} -> x in n1..n2 end)
  end

